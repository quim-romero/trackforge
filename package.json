{
  "name": "trackforge",
  "private": true,
  "version": "1.2.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "cy:run": "cypress run --browser chrome --headless",
    "test:e2e": "start-server-and-test dev http://localhost:5173 cy:run",
    "test:e2e:open": "cypress open",
    "e2e": "npm run test:e2e",
    "analyze": "ANALYZE=1 vite build && echo 'Bundle report: dist/stats.html'",
    "lhci:ci": "lhci autorun --config=.lighthouserc.json",
    "lh:report": "lhci autorun --collect.staticDistDir=dist --upload.target=filesystem --upload.outputDir=lhci"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@supabase/supabase-js": "^2.54.0",
    "chart.js": "^4.5.0",
    "dayjs": "^1.11.13",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.539.0",
    "react": "^19.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-router-dom": "^7.8.0",
    "zod": "^4.0.15",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@lhci/cli": "^0.15.1",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.10.3",
    "cypress": "^14.5.4",
    "cypress-axe": "^1.6.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.3",
    "start-server-and-test": "^2.0.13",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.1.0"
  }
}
